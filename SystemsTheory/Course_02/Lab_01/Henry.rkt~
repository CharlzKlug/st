#lang racket
(require math/array)

(define (make-round bx)
  (make-array (vector bx) 1))

(define a 
  (array #[#[2 1 4 3]
           #[3 1 4 2]
           #[4 1 3 2]
           #[4 2 3 1]]))

(define b 
  (array #[#[1 4 3 2]
           #[1 2 3 4]
           #[4 2 1 3]
           #[3 2 1 4]]))

(define solution
  (array #[4 1 1 1]))